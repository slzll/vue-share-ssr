{
  "name": "vue-share-ssr",
  "version": "0.0.3",
  "keywords": [
    "vue-share-ssr",
    "social-share.js",
    "vue share ssr",
    "vue ssr",
    "nuxtjs",
    "social share"
  ],
  "description": "vue social share based on social-share.js and it supports SSR",
  "main": "dist/share.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:spa": "vue-cli-service build --target lib --name share src/index.js",
    "build:ssr": "webpack",
    "build": "npm run build:spa && npm run build:ssr",
    "prepare": "npm run build",
    "publish": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slzll/vue-share-ssr.git"
  },
  "author": "slzll",
  "license": "MIT",
  "dependencies": {
    "qrcode": "^1.4.2",
    "social-share.js": "^1.0.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@vue/cli-service": "^3.11.0",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.9"
  }
}
